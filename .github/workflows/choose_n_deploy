  deploy:
    runs-on: self-hosted
    needs: choose_best_model
    if: always()
    steps:
      - name: Checkout repository
        uses: actions/checkout@v3
      - name: Start deploy
        env:
          DB_USERNAME: ${{secrets.DB_USERNAME}}
          DB_PASSWORD: ${{secrets.DB_PASSWORD}}
        run: curl "http://localhost:5000/update"
